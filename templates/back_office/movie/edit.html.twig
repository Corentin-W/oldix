{% extends 'back_office/backoffice_base.html.twig' %}


{% block body %}
	<div class="container mt-5">

		{{form_start(form)}}
		<div class="row">
			<div class="col-6">
				{{form_row(form.name)}}
			</div>
			<div class="col-6">
				{{form_row(form.link)}}
			</div>
		</div>
		<div class="row d-flex justify-content-center">
			<div class="col-6 ">
				{{form_row(form.language)}}
			</div>
		</div>
		<div class="row d-flex justify-content-center">
			<div class="col-3">
				{{form_row(form.thematic)}}
			</div>
			<div class="col-3">
				{{form_row(form.category)}}
			</div>
		</div>
		<div class="d-flex justify-content-center">
			<button class="btn btn-primary rounded">Create</button>
		</div>
		{{form_end(form)}}
	</div>
{% endblock %}
